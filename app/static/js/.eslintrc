{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prefer-stateless-function": 0,
    "react/no-multi-comp": 0,
    "react/forbid-prop-types": 0,
  },
  settings: {
    'import/resolver': 'webpack',
  },
}
